{
    "name": "endroid/property-access",
    "description": "Endroid Property Access",
    "keywords": ["endroid", "property", "access"],
    "homepage": "https://github.com/endroid/property-access",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "https://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/expression-language": ">=2.3",
        "symfony/property-access": ">=2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\PropertyAccess\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\PropertyAccess\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
