{
    "name": "endroid/property-access",
    "description": "Endroid Property Access",
    "keywords": ["endroid", "property", "access", "php"],
    "homepage": "https://github.com/endroid/property-access",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": ">=7.2",
        "symfony/expression-language": "^3.4||^4.4||^5.0",
        "symfony/property-access": "^3.4||^4.4||^5.0"
    },
    "require-dev": {
        "endroid/quality": "dev-master"
    },
    "suggest": {
        "roave/security-advisories": "Avoids installation of package versions with vulnerabilities",
        "symfony/security-checker": "Checks your composer.lock for vulnerabilities"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\PropertyAccess\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\PropertyAccess\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "minimum-stability": "beta",
    "config": {
        "sort-packages": true
    }
}
